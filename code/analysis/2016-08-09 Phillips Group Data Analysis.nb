        RowBox[{"-", "\[Beta]\[Epsilon]"}]], 
       FractionBox["1", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "+", 
           OverscriptBox["c", "~"]}], ")"}], "2"]]}], "+", 
      RowBox[{"2", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"-", "\[Beta]\[Epsilon]"}]], 
       FractionBox[
        FractionBox[
         OverscriptBox["c", "~"], 
         OverscriptBox["K", "~"]], 
