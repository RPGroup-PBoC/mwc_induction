Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["p", "A"], "(", 
    OverscriptBox["c", "~"], ")"}], "=", 
   FractionBox["1", 
    RowBox[{"1", "+", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"-", "\[Beta]\[Epsilon]"}]], 
      FractionBox[
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "+", 
          FractionBox[
           OverscriptBox["c", "~"], 
           OverscriptBox["K", "~"]]}], ")"}], "2"], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "+", 
          OverscriptBox["c", "~"]}], ")"}], "2"]]}]}]]}], TraditionalForm]],
 FormatType->"TraditionalForm"]], "TextNumbered",
 CellChangeTimes->{{3.6807259105746465`*^9, 3.680725933730553*^9}, {
