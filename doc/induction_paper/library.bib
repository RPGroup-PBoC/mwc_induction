pages = {103--33},
pmid = {22224598},
publisher = {Annual Reviews},
title = {{Allostery and the Monod-Wyman-Changeux model after 50 years.}},
url = {http://www.annualreviews.org/doi/full/10.1146/annurev-biophys-050511-102222},
volume = {41},
year = {2012}
}
@article{Evans1985,
abstract = {The mitochondrial NAD+: isocitrate dehydrogenase from Rhodosporidium toruloides CBS 14 had a molecular radius of 191 800 daltons, a pH optimum of 7.8, and an apparent Kms for isocitrate and NAD+ of 109 and 365 $\mu$M, respectively. The enzyme was absolutely dependent on AMP for activity and showed sigmoidal activation curves with respect to isocitrate concentration. ATP was shown to be a potent inhibitor even in the presence of saturating AMP and isocitrate concentrations. NADH was the most potent inhibitor, followed by L-glutamate and $\alpha$-ketoglutarate, and less strongly by citrate and oxaloacetate. The inhibition by L-glutamate and $\alpha$-ketoglutarate was noncompetitive, with respect to isocitrate, and indicated that the enzyme could be very susceptible to regulation by the products of organic nitrogen metabolism in this yeast. Inhibition by citrate and more so the strong inhibition by NADH illustrated the interplay between the inactivation of NAD+: isocitrate dehydrogenase and the steps leading to the biosynthes...},
