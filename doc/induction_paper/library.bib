file = {:C$\backslash$:/Users/Tal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Otero et al. - 2011 - Different Effects of Mg and Zn on the Two Sites for Alkylammonium Compounds in Pseudomonas aeruginosa Phosphorylch.pdf:pdf},
