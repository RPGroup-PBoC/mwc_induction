isbn = {0027-8424 (Print)$\backslash$n0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Cyclic AMP Receptor Protein,Cyclic AMP Receptor Protein: metabolism,DNA, Bacterial,DNA, Bacterial: chemistry,Escherichia coli,Escherichia coli: genetics,Escherichia coli: growth {\&} development,Gene Expression Regulation, Bacterial,Genes, Bacterial,Lac Operon,Nucleic Acid Conformation,Promoter Regions, Genetic,Repressor Proteins,Repressor Proteins: genetics,Repressor Proteins: metabolism,Systems Biology,Transcription, Genetic},
