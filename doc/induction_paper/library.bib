journal = {Current Opinion in Structural Biology},
keywords = {KNF,Koshland, Nemethy and Filmer,MD,MWC,Monod, Wyman and Changeux,Nemethy and Filmer,PDB,Protein Data Bank,Wyman and Changeux,molecular dynamics,rms,root mean square},
month = {dec},
