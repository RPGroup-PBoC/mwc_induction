file = {:C$\backslash$:/Users/Tal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wales et al. - 1999 - Divergent allosteric patterns verify the regulatory paradigm for aspartate transcarbamylase.pdf:pdf},
issn = {0022-2836},
journal = {J. Mol. Biol.},
keywords = {allosteric regulation,aspartate transcarbamoylase,cambridge,co-operativity,department,e,glaser,herbicola,inc,l,ma 02142,madison,metabolix,present addresses,s,sequence alignment,usa},
number = {5},
