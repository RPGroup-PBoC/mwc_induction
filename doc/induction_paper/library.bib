pages = {1247--54},
pmid = {8638105},
title = {{Crystal structure of the lactose operon repressor and its complexes with DNA and inducer.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/8638105},
volume = {271},
year = {1996}
}
