file = {:C$\backslash$:/Users/Stephanie/Documents/Lab/Readings/Papers/Transcription and Gene Regulation/Kuhlman - 2007 - Combinatorial transcriptional control of the lactose operon of Escherichia coli.pdf:pdf},
isbn = {0027-8424 (Print)$\backslash$n0027-8424 (Linking)},
