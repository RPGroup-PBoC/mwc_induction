journal = {Proc. Natl. Acad. Sci. U. S. A.},
keywords = {Adult,Allosteric Site,Allosteric Site: genetics,Catalytic Domain,Catalytic Domain: genetics,Hemoglobins,Hemoglobins: chemistry,Hemoglobins: genetics,Humans,Kinetics,Models, Molecular,Molecular,Mutation,Mutation: genetics,Protein Conformation,Recombinant Proteins,Recombinant Proteins: genetics},
