@article{Gunawardena2014,
abstract = {Michaelis and Menten introduced to biochemistry the idea of time-scale separation, in which part of a system is assumed to be operating sufficiently fast compared to the rest so that it may be taken to have reached a steady state. This allows, in principle, the fast components to be eliminated, resulting in a simplified description of the system's behaviour. Similar ideas have been widely used in different areas of biology, including enzyme kinetics, protein allostery, receptor pharmacology, gene regulation and post-translational modification. However, the methods used have been independent and ad hoc. In the present study, we review the use of time-scale separation as a means to simplify the description of molecular complexity and discuss recent work setting out a single framework that unifies these separate calculations. The framework offers new capabilities for mathematical analysis and helps to do justice to Michaelis and Menten's insights about individual enzymes in the context of multi-enzyme biological systems.},
author = {Gunawardena, Jeremy},
doi = {10.1111/febs.12532},
file = {:C$\backslash$:/Users/Tal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gunawardena - 2014 - Time-scale separation - Michaelis and Menten's old idea, still bearing fruit.pdf:pdf},
issn = {1742464X},
journal = {FEBS J.},
keywords = {Matrix-Tree Theorem,algebraic elimination,linear framework,quasi-steady state assumption,time-scale separation},
number = {2},
pages = {473--488},
pmid = {24103070},
title = {{Time-scale separation - Michaelis and Menten's old idea, still bearing fruit}},
volume = {281},
year = {2014}
}
@article{Cockrell2013,
abstract = {For nearly 60 years, the ATP activation and the CTP inhibition of Escherichia coli aspartate transcarbamoylase (ATCase) has been the textbook example of allosteric regulation. We present kinetic data and five X-ray structures determined in the absence and presence of a Mg(2+) concentration within the physiological range. In the presence of 2 mM divalent cations (Mg(2+), Ca(2+), Zn(2+)), CTP does not significantly inhibit the enzyme, while the allosteric activation by ATP is enhanced. The data suggest that the actual allosteric inhibitor of ATCase in vivo is the combination of CTP, UTP, and a divalent cation, and the actual allosteric activator is a divalent cation with ATP or ATP and GTP. The structural data reveals that two NTPs can bind to each allosteric site with a divalent cation acting as a bridge between the triphosphates. Thus, the regulation of ATCase is far more complex than previously believed and calls many previous studies into question. The X-ray structures reveal that the catalytic chains undergo essentially no alternations; however, several regions of the regulatory chains undergo significant structural changes. Most significant is that the N-terminal region of the regulatory chains exists in different conformations in the allosterically activated and inhibited forms of the enzyme. Here, a new model of allosteric regulation is proposed.},
author = {Cockrell, Gregory M. and Zheng, Yunan and Guo, Wenyue and Peterson, Alexis W. and Truong, Jennifer K. and Kantrowitz, Evan R.},
doi = {10.1021/bi401205n},
file = {:C$\backslash$:/Users/Tal/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cockrell et al. - 2013 - New paradigm for allosteric regulation of escherichia coli aspartate transcarbamoylase.pdf:pdf},
issn = {00062960},
journal = {Biochemistry},
number = {45},
pages = {8036--8047},
pmid = {24138583},
title = {{New paradigm for allosteric regulation of escherichia coli aspartate transcarbamoylase}},
volume = {52},
year = {2013}
}
