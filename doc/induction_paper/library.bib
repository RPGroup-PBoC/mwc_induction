journal = {Proc. Natl. Acad. Sci.},
keywords = {Enzymes,Enzymes: metabolism,Kinetics,Protein Binding,Substrate Specificity},
month = {mar},
number = {12},
pages = {4391--4396},
pmid = {24616494},
title = {{Role of substrate unbinding in Michaelis-Menten enzymatic reactions}},
volume = {111},
year = {2014}
}
