pmid = {8045263},
title = {{Quality and position of the three lac operators of E. coli define efficiency of repression.}},
volume = {13},
year = {1994}
}
@article{Vilar2003,
abstract = {DNA looping participates in transcriptional regulation, for instance, by allowing distal binding sites to act synergistically. Here, we study this process and compare different regulatory mechanisms based on repression with and without looping. Within a simple mathematical model for the lac operon, we show that regulation based on DNA looping, in addition to increasing the repression level, can reduce the fluctuations of transcription and, at the same time, decrease the sensitivity to changes in the number of regulatory proteins. Looping is thus able to circumvent some of the constraints inherent to mechanisms based solely on binding to a single operator site and provides a mechanism to regulate the average properties of transcription and its fluctuations.},
author = {Oehler, S and Amouyal, M and Kolkhof, P and von Wilcken-Bergmann, B and M{\"{u}}ller-Hill, B},
journal = {J Mol Biol},
number = {5},
pages = {981--989},
pmid = {12927535},
title = {{DNA looping and physical constraints on transcription regulation.}},
volume = {331},
year = {2003}
}
@article{Kern2003,
abstract = {The biomolecular conformational changes often associated with allostery are, by definition, dynamic processes. Recent publications have disclosed the role of pre-existing equilibria of conformational substates in this process. In addition, the role of dynamics as an entropic carrier of free energy of allostery has been investigated. Recent work thus shows that dynamics is pivotal to allostery, and that it constitutes much more than just the move from the ‘T'-state to the ‘R'-state. Emerging computational studies have described the actual pathways of allosteric change.},
author = {Kern, Dorothee and Zuiderweg, Erik RP},
doi = {10.1016/j.sbi.2003.10.008},
